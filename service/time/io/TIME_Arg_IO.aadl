package TIME_Arg_IO
public
	with TIME_Interface;
	
	--------all feature groups can add "feature group [type];" corresponding to its data type
	feature group GetTimeArgs
		features
			a: feature group TIME_Interface::P_GetTime;
	end GetTimeArgs;
	
	feature group GetTimeArgsR
		features
			a: feature group TIME_Interface::R_GetTime;
		inverse of GetTimeArgs
	end GetTimeArgsR;
end TIME_Arg_IO;